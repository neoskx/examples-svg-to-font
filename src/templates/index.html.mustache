<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <title>Icon Font generator</title>
    <meta name="description" content="An Icon Font Generator">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="vendor/bootstrap/css/bootstrap.min.css" >
    <link rel="stylesheet" href="vendor/bootstrap/css/bootstrap-theme.min.css" >
    <link rel="stylesheet" href="assets/css/app.css">
    <link rel="stylesheet" href="icon-font/css/icon-font.css"></head>
<body>
    <div class="jumbotron" id="content" tabindex="-1">
      <div class="container">
        <h1>{{ fontName }}</h1>
        <p>{{ description }}</p>
      </div>
    </div>
    <div class="container">
      <div clas="row">
      {{#glyphsMap}}
        <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6">
          <div class="panel panel-default">
            <div class="panel-heading">
              <i class="{{basicIconClass}} {{basicIconClass}}-{{name}}"></i>
              <span>{{basicIconClass}} {{basicIconClass}}-{{name}}</span>
            </div>
            <div class="panel-body">
              <div class="highlight">
                <pre><code class="html">&lt;i class="{{basicIconClass}} {{basicIconClass}}-{{name}}"&gt;&lt;/i&gt;</code></pre>
              </div>
            </div>
          </div>
        </div>
      {{/glyphsMap}}
      </div>
    </div>
    <script src="vendor/jquery/jquery-3.1.0.min.js"></script>
    <script src="vendor/bootstrap/js/bootstrap.min.js"></script>
    <script src="assets/js/app.js"></script>
</body>
</html>
